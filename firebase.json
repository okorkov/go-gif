{
  "hosting": {
    "public": "build",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "headers": [ {
      "source": "**",
      "headers": [ {
        "key": "Cross-Origin-Embedder-Policy",
        "value": "require-corp"
      } ]
    }, {
      "source": "**",
      "headers": [ {
        "key": "Cross-Origin-Opener-Policy",
        "value": "same-origin"
      } ]
    } ],
    "rewrites": [
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  },
  "emulators": {
    "hosting": {
      "port": 3333,
      "host": "127.0.0.1"
    }
  }
}
